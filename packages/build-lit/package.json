			"./styles": "./styles.d.ts",
			"./package.json": "./package.json"
		}
	},
	"peerDependencies": {
		"@lit/localize": "^0.12.0",
		"karma": "^6.4.1",
		"sass": "^1.58.1",
		"typescript": "~5.2.2"
	},
	"peerDependenciesMeta": {
		"@lit/localize": {
			"optional": true
		},
		"karma": {
			"optional": true
		},
		"sass": {
			"optional": true
		}
	},
	"dependencies": {
		"@snuggery/architect": "^0.8.0",
		"@snuggery/core": "^0.6.1",
		"@snuggery/snuggery": "^0.10.0",
		"@xmldom/xmldom": "^0.8.2",
		"chokidar": "^3.5.1",
		"esbuild-wasm": "^0.17.4",
		"fast-glob": "^3.2.12",
		"parse5": "^7.1.2",
		"resolve.exports": "^2.0.0",
		"source-map": "0.6.1"
	},
	"optionalDependencies": {
		"esbuild": "^0.17.4"
	},
	"devDependencies": {
		"@lit/localize": "^0.12.0",
		"@types/karma": "^6.3.3",
		"@types/node": "18.11.18",
		"karma": "^6.4.1",
		"sass": "^1.58.1",
		"typescript": "~5.2.2"
	}
